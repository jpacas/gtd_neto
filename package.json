{
  "name": "gtd_neto",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "server.js",
  "scripts": {
    "build:css": "npx tailwindcss -i ./src/styles.css -o ./public/css/styles.css --minify",
    "watch:css": "npx tailwindcss -i ./src/styles.css -o ./public/css/styles.css --watch",
    "build": "npm run build:css",
    "start": "node server.js",
    "dev": "node --watch server.js",
    "dev:all": "npm run watch:css & npm run dev",
    "lint": "node scripts/lint.mjs",
    "format:check": "node scripts/format-check.mjs",
    "test": "node --test",
    "check": "npm run lint && npm run format:check && npm run test",
    "check:supabase:conflict": "node scripts/check-supabase-conflict-target.mjs"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.95.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "sanitize-html": "^2.17.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.24",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19"
  }
}
